{
    "manifest_version": 3,
    "name": "YouTube Video Notes",
    "version": "1.0",
    "description": "Generate notes from YouTube videos.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
           
            "128": "icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
       
        "128": "icon128.png"
    }
}
